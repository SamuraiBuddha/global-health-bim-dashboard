# Global Health BIM Dashboard Environment Configuration
# Copy this file to .env and update with your actual values

# PostgreSQL Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=global_health_dashboard

# InfluxDB Time Series Database
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=changeme
INFLUXDB_ORG=global_health
INFLUXDB_BUCKET=metrics
INFLUXDB_TOKEN=your-super-secret-auth-token

# Neo4j Graph Database
NEO4J_USER=neo4j
NEO4J_PASSWORD=changeme

# Kong API Gateway
KONG_PG_PASSWORD=changeme

# n8n Workflow Automation
N8N_ENCRYPTION_KEY=your-n8n-encryption-key
N8N_USER_MANAGEMENT_JWT_SECRET=your-jwt-secret

# Cesium Ion Token (for 3D terrain visualization)
# Get your free token at: https://cesium.com/ion/tokens
CESIUM_ION_TOKEN=your_cesium_ion_token_here

# Grafana Monitoring
GRAFANA_USER=admin
GRAFANA_PASSWORD=changeme

# LightRAG Configuration (optional)
LIGHTRAG_HOST=lightrag
LIGHTRAG_PORT=8000
EMBEDDING_SERVICE=http://ollama:11434

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8080
REACT_APP_WS_URL=ws://localhost:8081
REACT_APP_UNREAL_STREAM_URL=http://localhost:8090

# API Service Configuration
NODE_ENV=development
PORT=8082
